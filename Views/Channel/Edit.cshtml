@model LetThereBeVoice.Models.Channel

<h2>Edit Channel</h2>

<form method="post">
    <input asp-for="ChannelID" type="hidden" />
    <label>Channel Name:</label>
    <input asp-for="ChannelName" class="form-control" />
    <br />
    <button type="submit" class="btn btn-primary">Save Changes</button>
    <a href="/Channel/List?serverId=@Model.ServerID" class="btn btn-secondary">Cancel</a>
</form>
